<script>
  document.title = 'Individual Character Name Information Edit Page';
</script>
<p>
  edit character name information
</p>

<div>
  <form
    action="/user/character_name/<%=character_name.id%>/edit?_method=put"
    method="post"
  >
    <div>
      <label for="ID">ID : </label>
      <input
        type="text"
        name="ID"
        class="form-control"
        placeholder="ID"
        value="<%=character_name.ID%>"
      />
    </div>
    <div>
      <label for="Name">Name : </label>
      <input
        type="text"
        name="Name"
        class="form-control"
        placeholder="Name"
        value="<%=character_name.Name%>"
      />
    </div>
    <div>
      <label for="Level">Level : </label>
      <input
        type="text"
        name="Level"
        class="form-control"
        placeholder="Level"
        value="<%=character_name.Level%>"
      />
    </div>
    <div class="form-group">
      <label for="Job">Job :</label>
      <select class="form-control" name="Job">
        <option value="<%=job_default._id%>"><%=job_default.Name%></option>
        <% character_jobs.forEach(character_job => { %>
        <option
          label="<%= character_job.ID %>.<%= character_job.Name %>"
          value="<%= character_job.id %>"
        >
          <% }) %>
        </option></select
      >
    </div>

    <a href="/user/character_name/<%=character_name.id%>">
      Go Back To Individual Character Name Information Detail</a
    ><br />
    <a href="/user/character_name">
      Go Back To Character Name Information List</a
    ><br />
    <button type="submit">Update Character Information</button>
  </form>
</div>
<br />
