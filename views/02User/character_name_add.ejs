<script>
  document.title = 'Add Character Name Page';
</script>

<h3>
  Add Character Genernal Name Info
</h3>

<form action="/user/character_name/new" method="post">
  <div class="form-group">
    <label>ID</label>
    <input type="text" class="form-control" name="ID" />
  </div>
  <div class="form-group">
    <label>Name</label>
    <input type="text" class="form-control" name="Name" />
  </div>
  <div class="form-group">
    <label>Level</label>
    <input type="text" class="form-control" name="Level" />
  </div>
  <div class="form-group">
    <label for="Job">Job</label>
    <select class="form-control" name="Job">
      <% character_jobs.forEach(character_job => { %>
      <option
        label="<%= character_job.ID %>.<%= character_job.Name %>"
        value="<%= character_job.id %>"
      >
        <% }) %>
      </option></select
    >
  </div>
  <button type="submit" class="btn btn-primary">Submit</button><br />
  <a href="/user/character_name"> Go Back To Character Name List</a>
</form>
